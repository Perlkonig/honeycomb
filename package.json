{
  "name": "honeycomb-grid",
  "version": "4.0.0-alpha.5",
  "description": "Create hexagon grids easily",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "benchmark": "ts-node --project tsconfig.node.json benchmarks",
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint . --fix",
    "postinstall": "husky install",
    "prerelease": "npm run lint && npm test && NODE_ENV=production npm run build",
    "preview": "vite preview",
    "release": "git add --patch && standard-version --commit-all",
    "test:watch": "npm test -- --watch",
    "test": "jest"
  },
  "author": "Abbe Keultjes <flauwekeul@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@svgdotjs/svg.js": "3.1.2",
    "@types/jest": "28.1.5",
    "@types/node": "~16.11.43",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "benny": "3.7.1",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.5.3",
    "gitmoji-cli": "5.0.3",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "precise-commits": "1.0.2",
    "prettier": "2.7.1",
    "standard-version": "9.5.0",
    "ts-jest": "28.0.6",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vite": "3.0.4"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flauwekeul/honeycomb.git"
  },
  "keywords": [
    "grid",
    "hex",
    "hex-grid",
    "hexagon",
    "hexagon-grid",
    "tiles",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/flauwekeul/honeycomb/issues"
  },
  "homepage": "http://abbekeultjes.nl/honeycomb",
  "engines": {
    "node": ">=16"
  }
}
